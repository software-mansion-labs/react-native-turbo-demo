{
  "name": "react-native-web-screen",
  "version": "1.0.0",
  "description": "Native like web screen in React Native",
  "homepage": "https://github.com/software-mansion-labs/react-native-turbo-demo",
  "repository": "https://github.com/software-mansion-labs/react-native-turbo-demo",
  "author": "Bartek Fryz <bartlomiej.fryz@swmansion.com> (https://github.com/Fryzu)",
  "license": "MIT",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "web",
    "webview",
    "navigation"
  ],
  "source": "src/index",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "scripts": {
    "build": "bob build && yarn link",
    "clean": "rm -rf lib node_modules",
    "release": "sh ../../scripts/release.sh",
    "typescript": "tsc",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@react-navigation/core": "^7.7.0",
    "@react-navigation/native": "^7.0.19",
    "react-native-turbo": "*"
  },
  "peerDependencies": {
    "@react-navigation/core": ">=7.7.0",
    "@react-navigation/native": ">=7.0.19",
    "react": "*",
    "react-native": "*",
    "react-native-turbo": "*"
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
